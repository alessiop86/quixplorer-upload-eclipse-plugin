<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.jdt.ui.PackageExplorer">
         <command
               commandId="quixplorer_uploader.uploadcommand"
               label="Carica con QuiXplorer"
               mode="FORCE_TEXT"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="quixplorer_uploader.UploaderHandler"
            id="quixplorer_uploader.uploadcommand"
            name="Upload">
      </command>
   </extension>

</plugin>
